{
  "<global-gam-define-display-ad>": {
    "template": "./define-display-ad.marko"
  },
  "<global-gam-wallpaper-ad>": {
    "template": "./wallpaper-ad.marko",
    "@aliases": "array",
    "@position": "string"
  },
  "<global-gam-inject-ads>": {
    "template": "./inject-ads.marko",
    "@inject <inject>[]": {},
    "@detect-embeds": "boolean",
